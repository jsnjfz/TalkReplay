    "role": { "type": ["string", "null"] },
    "contentString": { "type": ["string", "null"] },
    "output": {},
    "toolUseResult": {
      "type": ["object", "null"],
      "additionalProperties": true
    },
    "contentItem": {
      "type": "object",
